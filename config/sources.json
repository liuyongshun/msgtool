    "maintenance": {
      "log_retention_days": 7,
      "cache_retention_days": 30,
      "auto_cleanup": {
        "enabled": true,
        "log_cleanup_time": "03:00",
        "cache_cleanup_time": "02:00",
        "cache_cleanup_day": "sunday",
        "note": "自动清理配置：日志每天3点清理，缓存每周日2点清理"
      }
    },
    "scheduler": {
      "enabled": true,
      "tasks": {
        "arxiv": {
          "enabled": true,
          "time": "21:52",
          "max_results": 20,
          "note": "每个分类最多抓取的论文数（优化token消耗：从50减少到20）",
          "translation_strategy": {
            "enabled": true,
            "selective_translation": true,
            "min_authors": 2,
            "note": "选择性翻译：只翻译作者数>=2的高质量论文"
          }
        },
        "hackernews": {
          "enabled": true,
          "time": ["10:20", "14:05", "17:20", "21:52"],
          "max_results": 50,
          "note": "控制原始抓取数量，AI筛选后的所有结果都会保存"
        },
        "rss": {
          "enabled": true,
          "time": ["10:20", "14:05", "17:20", "21:52"],
          "max_results": 20,
          "note": "每个RSS源最多抓取的条目数，AI筛选后的所有结果都会保存。每天从06:00到21:00，每2小时同步一次（共9次）。"
        },
        "github": {
          "enabled": true,
          "time": "21:52",
          "max_results": 100,
          "note": "控制每种查询类型的抓取上限，AI筛选后的所有结果都会保存"
        }
      }
    }
